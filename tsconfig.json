{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ],
      "@/components/*": [
        "./app/components/*"
      ],
      "@/services/*": [
        "./app/services/*"
      ],
      "@/utils/*": [
        "./app/utils/*"
      ],
      "@/types/*": [
        "./shared/types/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ]
}
